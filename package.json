{
  "name": "radishes",
  "version": "2.0.0-alpha.5",
  "private": true,
  "main": "./background.js",
  "author": "Linkontoask",
  "description": "Cross-platform copyright-free music platform",
  "scripts": {
    "bootstrap": "npx lerna@3.22.1 bootstrap",
    "inspect": "vue-cli-service inspect",
    "test": "lerna run test --stream",
    "test:unit": "vue-cli-service test:unit",
    "test:open": "cypress open",
    "test:run": "cypress run",
    "lint": "vue-cli-service lint && stylelint \"src/**/*.{css,less}\"",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,css,less}\"",
    "lint:style": "stylelint --fix \"src/**/*.{css,less}\" --cache --cache-location node_modules/.cache/stylelint/",
    "build": "cross-env NODE_ENV=production vue-cli-service build --mode browser",
    "build:electron": "node script/build.js --win",
    "build:electron:linux": "node script/build.js --linux",
    "build:electron:macos": "node script/build.js --mac",
    "build:api": "lerna exec --scope @radishes/api -- yarn run build",
    "dev": "concurrently -r \"yarn dev:api\" \"yarn generate:theme\" \"yarn dev:web\"",
    "dev:web": "cross-env NODE_ENV=development vue-cli-service serve --mode browser",
    "dev:api": "lerna exec --scope @radishes/api -- yarn run dev",
    "dev:electron": "concurrently -r \"yarn generate:theme\" \"vue-cli-service electron:serve --mode electron.dev\"",
    "dev:analyzer": "cross-env ANALYZER=analyzer vue-cli-service serve --mode browser",
    "generate:theme": "node ./script/theme.js",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "log": "conventional-changelog -p eslint -i CHANGELOG.md -s -r 0",
    "electron:generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten",
    "svgo": "svgo -f ./src/assets/svg",
    "serve": "vite --mode browser"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Linkontoask/radishes.git"
  },
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.{css,less}": [
      "stylelint \"src/**/*.{css,less}\"",
      "git add"
    ]
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@vueuse/core": "4.9.1",
    "ant-design-vue": "2.0.1",
    "axios": "^0.21.1",
    "dayjs": "^1.9.4",
    "electron": "12.2.3",
    "electron-builder": "^23.0.2",
    "electron-devtools-installer": "^3.1.0",
    "electron-icon-builder": "^1.0.2",
    "electron-log": "^4.3.1",
    "electron-store": "^6.0.1",
    "electron-updater": "^4.3.5",
    "js-base64": "^3.7.2",
    "js-cookie": "^2.2.1",
    "uuid": "^8.3.2",
    "v-easy-components": "2.0.0",
    "vant": "^3.0.6",
    "vue": "^3.3.7",
    "vue-i18n": "^9.1.7",
    "vue-router": "^4.2.5",
    "vuex": "^4.1.0",
    "vuex-persistedstate": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/chai": "^4.2.11",
    "@types/classnames": "^2.2.10",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/file-saver": "^2.0.1",
    "@types/js-cookie": "^2.2.6",
    "@types/jsmediatags": "^3.9.2",
    "@types/md5": "^2.2.1",
    "@types/mocha": "^5.2.4",
    "@types/normalize-url": "^4.2.0",
    "@types/portscanner": "^2.1.0",
    "@types/scriptjs": "^0.0.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/babel-plugin-jsx": "^1.0.3",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^2.0.0-0",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chalk": "^4.1.0",
    "classnames": "^2.2.6",
    "concurrently": "^5.3.0",
    "conventional-changelog-cli": "^2.1.1",
    "cross-env": "^7.0.2",
    "cross-spawn": "^7.0.3",
    "cypress": "^6.2.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-vue": "^7.0.0-0",
    "express": "^4.17.1",
    "file-saver": "^2.0.5",
    "github-markdown-css": "^4.0.0",
    "http-proxy-middleware": "^1.0.6",
    "husky": "^4.3.0",
    "lerna": "^3.22.1",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.5.0",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "music-metadata": "^7.5.2",
    "normalize-url": "^5.3.0",
    "portscanner": "^2.2.0",
    "prettier": "^1.19.1",
    "react-is": "^16.13.1",
    "scriptjs": "^2.5.9",
    "spectron": "^14.0.0",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^2.3.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.1.2",
    "svgo": "^1.3.2",
    "ts-loader": "^8.0.6",
    "typescript": "^5.2.2",
    "url-loader": "^4.1.1",
    "vite": "^4.5.0",
    "vite-plugin-importer": "^0.2.5",
    "vite-plugin-md2vue": "^1.1.1",
    "vue-cli-plugin-electron-builder": "^2.1.1",
    "vue-loader": "^16.1.2",
    "webpack-bundle-analyzer": "4.3.0"
  }
}